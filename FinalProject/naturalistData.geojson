{ "type": "Feature", "properties": { "id": 162922978, "dateObserved": "5/21/23", "dayObserved": null, "monthObserved": null, "yearObserved": null, "description": "Lone red wolf on left edge of field. I watched it for about 10 minutes through binoculars. It ran along the edge of the field stopping occasionally to look around. Toward the back of the field, as it was going into the brush, it looked like it pounced on something, then disappeared. Lone red wolf on left edge of field. I watched it for about 10 minutes through binoculars. It ran along the edge of the field stopping occasionally to look around. Toward the back of the field, as it was going into the brush, it looked like it pounced on something, then disappeared.", "latitude": 35.90720853, "longitude": -75.98615717, "field_9": null, "field_10": null, "field_11": null, "field_12": null, "field_13": null }, "geometry": { "type": "Point", "coordinates": [ -75.98615717, 35.90720853 ] } },
